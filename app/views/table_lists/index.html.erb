<section>

<ul class="breadcrumb">
  <li><%= link_to 'back', :back %></li>
</ul>

<div class="page-header">
  <h1>Preview</h1>
  <%= link_to 'download', report_list_table_list_path(@report_list, @report_list.table_lists.first, format: 'csv') %>
</div>

<div class="pagination pagination-centered">
  <ul>
    <% @table_lists.each_with_index do |table, index| %>
      <li><%= link_to index+1, report_list_table_list_path(@report_list, table) %></li>
    <% end %>
  </ul>
</div>

<% if @table_lists.first.present? %>
  <%= render partial: 'table', locals: { table_list: @table_lists.first } %>
<% end %>

</section>