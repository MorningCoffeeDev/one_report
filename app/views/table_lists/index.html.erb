<%= stylesheet_link_tag 'one_report/application', media: 'all' %>
<%= javascript_include_tag 'one_report/application' %>

<div class="ui segment"></div>

<div class="ui segment">
  <ul class="ui breadcrumb">
    <li class="section"><%= link_to 'back', :back %></li>
  </ul>
</div>


<div class="ui segment">



<div class="page-header">
  <h1>Preview</h1>
</div>

<div class="pagination pagination-centered">
  <ul>
    <% @table_lists.each_with_index do |table, index| %>
      <li><%= link_to index+1, report_list_table_list_path(@report_list, table) %></li>
    <% end %>
  </ul>
</div>

<% if @table_lists.first.present? %>
  <%= render partial: 'table', locals: { table_list: @table_lists.first } %>
<% end %>

</div>